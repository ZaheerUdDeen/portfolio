      </div>
      <div>
        <div class="text-3xl w-full font-bold">Soft Skills</div>
        <Skill
          name="Project Mangement"
          style="color: #232F3E; text-decoration: none; border-bottom: 2px solid #232F3E;"
        />

        <Skill
          name="Scrum Planing"
          style="color: #232F3E; text-decoration: none; border-bottom: 2px solid #232F3E;"
        />

        <Skill
          name="Canbin planing"
          style="color: #232F3E; text-decoration: none; border-bottom: 2px solid #232F3E;"
        />

        <Skill
        name="Business Coordination & Collabrations."
        style="color: #232F3E; text-decoration: none; border-bottom: 2px solid #232F3E;"
      />
      </div>
    </div>

